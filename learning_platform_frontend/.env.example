# Frontend configuration (.env file must be injected by orchestrator)
# Only REACT_APP_* vars are accessible at runtime in CRA

REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_KEY=

# API base URL (e.g., https://api.example.com). Empty -> same-origin relative paths
REACT_APP_API_BASE=

# WebSocket URL (e.g., wss://api.example.com/ws). Empty -> auto-derived from window.location
REACT_APP_WS_URL=

# Comma-separated feature flags (e.g., "analyticsV2,betaUI")
REACT_APP_FEATURE_FLAGS=

# Log level: error|warn|info|debug
REACT_APP_LOG_LEVEL=info

# Optional additional vars already supported by env.js
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=
REACT_APP_NODE_ENV=development
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_EXPERIMENTS_ENABLED=false
REACT_APP_NEXT_TELEMETRY_DISABLED=1
